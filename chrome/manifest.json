{
    "name": "GoodTabs",
    "description": "Do something good with GoodTabs",
    "version": "1.1.2",
    "icons": {
        "16": "data/assets/img/planet-earth.png",
        "48": "data/assets/img/planet-earth.png",
        "128": "data/assets/img/planet-earth.png"
    },
    "manifest_version": 3,
    "host_permissions": [
        "https://*/*",
        "http://*/*"
    ],
    "declarative_net_request": {
        "rule_resources": [{
            "id": "ruleset_1",
            "enabled": true,
            "path": "rule.json"
        }]
    },
    "permissions": [
        "topSites",
        "declarativeNetRequest",
        "declarativeNetRequestFeedback"
    ],
    "action": {
        "default_icon": {
            "16": "data/assets/img/planet-earth.png",
            "32": "data/assets/img/planet-earth.png",
            "48": "data/assets/img/planet-earth.png",
            "128": "data/assets/img/planet-earth.png"
        }
    },
    "chrome_url_overrides": {
        "newtab": "data/newtab.html"
    }
}